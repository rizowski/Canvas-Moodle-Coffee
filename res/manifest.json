{
    "name": "Canvas to Moodle",
	"description": "Simplifying your life, with just a simple install.",
    "version": "1.6.0",
	"manifest_version": 2,
	"icons" : {
            "128": "128v2.png"
        },
    "options_page" : "options.html",
    "content_scripts": [
        {
            "matches":[
                "https://*.instructure.com/*",
                "https://lms.neumont.edu/"
            ],
            "js":[
                "jquery.min.js", 
                "contentscript.js"
            ]
        }
    ],
    "permissions":[
        "https://*.instructure.com/*",
        "https://lms.neumont.edu/*"
    ],
    "web_accessible_resources":[
        "contentscript.js.map"
    ]
}
